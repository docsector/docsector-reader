{
  "name": "@docsector/docsector-reader",
  "version": "0.2.1",
  "description": "A documentation rendering engine built with Vue 3, Quasar v2 and Vite. Transform Markdown into beautiful, navigable documentation sites.",
  "productName": "Docsector Reader",
  "author": "Rodrigo de Araujo Vieira",
  "license": "MIT",
  "private": false,
  "readmeFilename": "README.md",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./config": "./docsector.config.js",
    "./quasar-factory": "./src/quasar.factory.js",
    "./i18n": "./src/i18n/helpers.js",
    "./boot/store": "./src/boot/store.js",
    "./boot/QZoom": "./src/boot/QZoom.js",
    "./boot/i18n": "./src/boot/i18n.js",
    "./boot/axios": "./src/boot/axios.js",
    "./src/css/*": "./src/css/*"
  },
  "bin": {
    "docsector": "bin/docsector.js"
  },
  "files": [
    "bin/",
    "src/",
    "public/",
    "docsector.config.js",
    "quasar.config.js",
    "index.html",
    "postcss.config.cjs",
    ".eslintrc.cjs",
    "jsconfig.json",
    "README.md",
    "LICENSE.md"
  ],
  "keywords": [
    "documentation",
    "docs",
    "vue3",
    "quasar",
    "vite",
    "markdown",
    "i18n",
    "docsector"
  ],
  "homepage": "https://github.com/docsector/docsector-reader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docsector/docsector-reader.git"
  },
  "bugs": {
    "url": "https://github.com/docsector/docsector-reader/issues"
  },
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/cli": "^2.5.0",
    "@quasar/extras": "^1.16.12",
    "axios": "^1.7.7",
    "core-js": "^3.6.5",
    "q-colorize-mixin": "^2.0.0-alpha.5",
    "quasar": "^2.16.6",
    "vue": "^3.5.13",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1",
    "hjson": "^3.2.2",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "prismjs": "^1.27.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^2.1.0",
    "@quasar/quasar-app-extension-qmediaplayer": "^2.0.0-beta.6",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": "^22 || ^20 || ^18",
    "npm": ">= 9"
  }
}
